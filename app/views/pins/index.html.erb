<div class="container">
  <div class="row">
    <div class="transitions-enabled" id="pins">
      <% @pins.each do |pin| %>
        <div class="box panel panel-default">
          <h2>
            <%= link_to pin.title, pin %>
          </h2>
          <p class="user">
            Submitted by
            <%= pin.user.email %>
          </p>
        </div>
      <% end %>
    </div>
  </div>
</div>